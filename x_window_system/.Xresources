/* ! run xrdb ~/.Xresources to apply */
/* ! name.Class.resource: value */

Xcursor.size: 16W
URxvt.urgentOnBell: true
URxvt.scrollBar: false
/* URxvt.font: xft:hack:size=10 */
URxvt.font: xft:SFMono-Regular:size=10
URxvt*depth: 32
URxvt.url-launcher: /usr/bin/google-chrome
URxvt.matcher.button: 1
!URxvt.perl-ext-common: default,matcher,vtwheel
URxvt.letterSpace: -1

!! Transparency (0-1):
*.alpha: 0.95

!! Set a default font and font size as below:
/* *.font: SFMono-Regular-12 */
*.font: SFMono-12
/* st.font: SFMono-12 */
/* st.font: SFMono-Regular-50 */
/* st.background: #151718 */

! st.termname: st-256color
! st.borderpx: 2

