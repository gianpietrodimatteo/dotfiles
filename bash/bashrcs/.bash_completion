#/usr/bin/env bash

# Source all files in completion folder
ls ~/.bash/completion/ |while read file; do
  source ~/.bash/completion/$file
done

# co - wrapper for configuraiton files
complete -W " \
  alias \
  custom  \
  function \
  variable \
  completion \
  bash \
  profile \
  input \
  git \
  repository \
  tmux \
  vim \
  map \
  resource \
  session \
  i3 \
  test \
  install \
  " co

